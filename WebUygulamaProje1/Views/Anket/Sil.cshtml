			<h2 class="text-primary">Anket Sil</h2>
			</hr>
		</div>


		<div class="mb-3">
			<label asp-for="AnketAdi" class="p-0"></label>
			<input asp-for="AnketAdi" disabled class="form-control" />
		</div>

		<div class="mb-3">
			<label asp-for="Soru1" class="p-0"></label>
			<input asp-for="Soru1" disabled class="form-control" />
