		<div class="mb-3">
			<label asp-for="Soru2" class="p-0"></label>
			<input asp-for="Soru2" disabled class="form-control" />
		</div>

		<div class="mb-3">
			<label asp-for="Soru3" class="p-0"></label>
			<input asp-for="Soru3" disabled class="form-control" />
		</div>

		<button type="submit" class="btn btn-lg btn-primary" style="width:250px">Anket Sil</button>

		<a asp-controller="Anket" asp-action="Index" class="btn btn-lg btn-primary" style="width:250px">
