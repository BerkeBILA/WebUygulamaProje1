@model AnketTuru

<form method="post">
	<input asp-for="Id" hidden />

	<div class="border p-3 mt-5">
		<div class="row pb-3">
			<h2 class="text-primary">Anket Türü Sil</h2>
		</div>

		<div class="mb-3">
			<label asp-for="Ad" class="p-0"></label>
			<input asp-for="Ad" disabled class="form-control" />
		</div>

		<button type="submit" class="btn btn-primary" style="width:250px">Anket Türü Sil</button>

		<a asp-controller="AnketTuru" asp-action="Index" class="btn btn-primary" style="width:250px">
			Anket Türlerine Geri Dön
		</a>
	</div>



</form>

